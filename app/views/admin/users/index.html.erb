<h3>Viewing Users</h3>
<table>
  <tr>
    <th>ID</th>
    <th>Login</th>
    <th>Name</th>
    <th>State</th>
    <th>Role</th>
    <th>Actions</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.login %></td>
      <td><%= user.name %></td>
      <td><%= user.state %></td>
      <td><%= user.role %></td>
      <td><%= link_to "edit", edit_admin_user_path(user) %></td>
    </tr>
  <% end %>
</table>